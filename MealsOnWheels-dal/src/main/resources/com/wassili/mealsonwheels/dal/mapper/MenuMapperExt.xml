<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wassili.mealsonwheels.dal.mapper.MenuMapperExt">
	<resultMap id="BaseResultMap" type="com.wassili.mealsonwheels.dal.model.Menu">
		<id column="MENU_ID" property="menuId" jdbcType="INTEGER"/>
		<result column="MENU_NAME" property="menuName" jdbcType="VARCHAR"/>
		<result column="MENU_IMAGE_URL" property="menuImageUrl" jdbcType="VARCHAR"/>
	</resultMap>
	<sql id="Base_Column_List">
		MENU_ID, MENU_NAME, MENU_IMAGE_URL
	</sql>
	<select id="queryMenu" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		select 
		<include refid="Base_Column_List"/>
		from MENU
		ORDER BY MENU_ID
	</select>
	
	
</mapper>